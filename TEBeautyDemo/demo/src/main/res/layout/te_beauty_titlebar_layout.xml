<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="50dp"
   >


    <ImageView
        android:id="@+id/te_title_layout_back_btn"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="10dp"
        android:src="@mipmap/te_beauty_titlebar_back_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/te_title_layout_title_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textColor="@color/te_beauty_color_FFFFFFFF"
        android:textSize="15sp"
        app:layout_constraintTop_toTopOf="@+id/te_title_layout_back_btn"
        app:layout_constraintBottom_toBottomOf="@+id/te_title_layout_back_btn"
        app:layout_constraintLeft_toRightOf="@+id/te_title_layout_back_btn"
         />


    <ImageView
        android:id="@+id/te_title_layout_resolution_btn"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="20dp"
        android:src="@mipmap/te_beauty_titlebar_more_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/te_title_layout_pick_btn"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/te_title_layout_pick_btn"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="20dp"
        android:src="@mipmap/te_beauty_titlebar_pick_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/te_title_layout_camera_switch_btn"
        app:layout_constraintTop_toTopOf="parent"

        />

    <ImageView
        android:id="@+id/te_title_layout_camera_switch_btn"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="20dp"
        android:src="@mipmap/te_beauty_titlebar_camera_switch_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>